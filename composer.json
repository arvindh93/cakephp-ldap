{
    "name": "arvindh93/cakephp-ldap-utility",
    "description": "Cakephp-ldap-utility plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.2.10 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "LdapUtility\\": "./plugins/LdapUtility/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LdapUtility\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
